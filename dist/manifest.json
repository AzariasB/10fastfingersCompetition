{
  "manifest_version": 3,
  "name": "10fastfingerCompetition",
  "description": "Knowing if there is a new competition in your langage",
  "version": "3.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "default_locale": "en",
  "options_page": "option.html",
  "icons": {
    "128": "pictures/big_icon.png"
  },
  "action": {
    "default_icon": {
      "16": "pictures/icon.png"
    }
  },
  "permissions": [
    "webRequest",
    "storage",
    "alarms",
    "activeTab",
    "tabs",
    "notifications"
  ],
  "host_permissions": ["*://10fastfingers.com/"]
}
