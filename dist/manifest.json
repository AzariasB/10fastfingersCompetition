{
	"background": {
		"persistent": true,
		"page": "background.html"
	},
	"manifest_version": 2,
	"default_locale": "en",
	"options_page": "option.html",
	"name": "10fastfingerCompetition",
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"version": "2.2",
	"icons": {
		"128": "pictures/big_icon.png"
	},
	"description": "Knowing if there is a new competition in your langage",
	"browser_action": {
		"default_icon": "pictures/icon.png"
	},
	"permissions": [ "storage", "alarms", "activeTab", "tabs", "notifications", "*://10fastfingers.com/" ]
}
